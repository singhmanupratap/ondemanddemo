    <script src="content/scripts/vendor.js?ver=1.2.0.1"></script>
    <script src="content/scripts/app.js?ver=1.2.0.1"></script>
</head>
<body>
    <div class="layout">
        <div class="layout-row">
            <header>
                <nav class="navbar navbar-inverse">
